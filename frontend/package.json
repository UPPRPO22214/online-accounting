{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate-api": "openapi-ts",
    "preview": "vite preview",
    "build": "tsc -b && vite build",
    "test:dev": "vitest",
    "test:check": "vitest --run",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier -w ./src",
    "all-checks": "bun run lint:check && bun run test:check && bun run build",
    "commit": "bun run prettier && bun run all-checks && git add -A && git commit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
