    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/node": "^24.5.2",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "eslint-plugin-zod-x": "^1.7.0",
    "immer": "^10.1.3",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "tailwindcss": "^4.1.13",
    "wouter": "^3.7.1",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@hey-api/openapi-ts": "0.89.0",
